{
  "name": "@startupjs/utils",
  "type": "module",
  "version": "0.57.0-canary.26",
  "description": "Isomorphic utils for internal cross-package usage",
  "main": "index.js",
  "exports": {
    "./mockBrowserify": {
      "node": "./mockBrowserify.server.cjs",
      "default": "./mockBrowserify.js"
    },
    "./forceMockBrowserify": "./forceMockBrowserify.js",
    "./isServer": "./isServer.js",
    "./isExpo": {
      "node": "./isExpo.server.js",
      "default": "./isExpo.js"
    },
    "./openAuthSessionAsync": {
      "node": "./openAuthSessionAsync.server.js",
      "default": "./openAuthSessionAsync.js"
    },
    "./getLinkingUri": {
      "node": "./getLinkingUri.server.js",
      "default": "./getLinkingUri.js"
    },
    "./reloadAppAsync": {
      "node": "./reloadAppAsync.server.js",
      "default": "./reloadAppAsync.js"
    },
    "./isWeb": "./isWeb.js",
    "./isDevelopment": "./isDevelopment.js",
    "./useIsomorphicLayoutEffect": "./useIsomorphicLayoutEffect.js",
