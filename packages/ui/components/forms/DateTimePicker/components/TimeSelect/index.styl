.cell
  height 5u
  width 20u
  align-items center
  justify-content center
  align-self center

  +from($UI.media.tablet)
    width 12u

.cellActive
  background-color var(--colors-primary, $UI.colors.primary)

.cellHover
  background-color var(--colors-primaryLighter, $UI.colors.primaryLighter)

.labelActive
  color var(--colors-white, $UI.colors.white)

:export
  media: $UI.media
