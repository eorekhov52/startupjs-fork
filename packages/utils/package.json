{
  "name": "@startupjs/utils",
  "type": "module",
  "version": "0.56.0-alpha.5",
  "description": "Isomorphic utils for internal cross-package usage",
  "main": "index.js",
  "exports": {
    "./mockBrowserify": {
      "node": "./mockBrowserify.server.cjs",
      "default": "./mockBrowserify.js"
    },
    "./forceMockBrowserify": "./forceMockBrowserify.js",
    "./isServer": "./isServer.js",
    "./isExpo": {
      "node": "./isExpo.server.js",
      "default": "./isExpo.js"
    },
    "./isWeb": "./isWeb.js",
    "./isDevelopment": "./isDevelopment.js",
    "./useIsomorphicLayoutEffect": "./useIsomorphicLayoutEffect.js",
    "./axios": {
      "node": "./axios.server.cjs",
      "default": "./axios.cjs"
    },
    "./BASE_URL": {
      "node": "./BASE_URL.server.js",
      "default": "./BASE_URL.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "expo-linking": "*",
    "react": "*"
  },
  "peerDependenciesMeta": {
    "expo-linking": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5"
  }
}
