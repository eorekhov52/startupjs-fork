{
  "name": "@startupjs/styleguide",
  "version": "0.55.0",
  "sideEffects": [
    "*.css",
    "*.styl"
  ],
  "exports": {
    "playground.plugin1.plugin": "./main/pages/PPlaygroundPlugins/plugin1.plugin.js"
  },
  "private": true,
  "scripts": {
    "start": "startupjs start",
    "android": "react-native run-android && (npm run adb || true) && npm run log-android",
    "android-release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "ios-release": "react-native run-ios --configuration Release",
    "metro": "react-native start --reset-cache",
    "web": "startupjs web",
    "server": "startupjs server",
    "adb": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:3010 tcp:3010",
    "log-android": "hash ccze 2>/dev/null && npm run log-android-color || (echo \"WARNING! Falling back to plain logging. For colored logs install ccze - brew install ccze\" && react-native log-android)",
    "log-android-color": "react-native log-android | ccze -m ansi -C -o nolookups",
    "build": "startupjs build --async",
    "start-production": "startupjs start-production",
    "test-e2e": "DB_READONLY=true DB_LOAD_SNAPSHOT=test/snapshots/test.db startupjs test-e2e test/e2e",
    "codegen-test-e2e": "startupjs codegen-test-e2e http://localhost:3000/"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@invertase/react-native-apple-authentication": "^2.1.0",
    "@react-native-clipboard/clipboard": "^1.7.0",
    "@react-native-community/push-notification-ios": "^1.8.0",
    "@react-native-cookies/cookies": "^6.0.5",
    "@react-native-google-signin/google-signin": "^6.0.0",
    "@react-native-picker/picker": "2.5.1",
    "@startupjs/2fa": "^0.56.0-alpha.0",
    "@startupjs/2fa-manager": "^0.56.0-alpha.0",
    "@startupjs/2fa-push-notification-provider": "^0.56.0-alpha.0",
    "@startupjs/2fa-totp-authentication": "^0.56.0-alpha.0",
    "@startupjs/2fa-totp-authentication-provider": "^0.56.0-alpha.0",
    "@startupjs/auth": "^0.56.0-alpha.0",
    "@startupjs/auth-apple": "^0.56.0-alpha.0",
    "@startupjs/auth-azuread": "^0.56.0-alpha.0",
    "@startupjs/auth-common": "^0.56.0-alpha.0",
    "@startupjs/auth-facebook": "^0.56.0-alpha.0",
    "@startupjs/auth-google": "^0.56.0-alpha.0",
    "@startupjs/auth-idg": "^0.56.0-alpha.0",
    "@startupjs/auth-linkedin": "^0.56.0-alpha.0",
    "@startupjs/auth-local": "^0.56.0-alpha.0",
    "@startupjs/docs": "^0.56.0-alpha.0",
    "@startupjs/i18n": "^0.56.0-alpha.0",
    "@startupjs/mdx": "^0.56.0-alpha.0",
    "@startupjs/push-notifications": "^0.56.0-alpha.0",
    "@startupjs/recaptcha": "^0.56.0-alpha.0",
    "@startupjs/scrollable-anchors": "^0.56.0-alpha.0",
    "@startupjs/serve-static-promo": "^0.56.0-alpha.0",
    "@startupjs/ui": "^0.56.0-alpha.0",
    "axios": "^0.21.1",
    "js-cookie": "^2.2.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-native": "0.63.5",
    "react-native-fbsdk": "^3.0.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-pager-view": "6.2.2",
    "react-native-push-notification": "^7.3.0",
    "react-native-restart": "^0.0.22",
    "react-native-svg": "^14.0.0",
    "react-native-webview": "10.10.2",
    "startupjs": "^0.56.0-alpha.0",
    "text-encoding-polyfill": "^0.6.7"
  }
}
